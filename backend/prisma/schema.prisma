generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Country {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  countryName String  @unique
  callingCode  String?
  capital String?
  population String?
  currency String?
  currencyShortName String?
  flag String?
  avarageCurrency String?
}