generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Country {
  id    Int     @id @default(autoincrement())
  countryName String  @unique
  callingCode  String?
  capital String?
  population String?
  currency String?
  flag String?
  avarageCurrency String?
}